{
  "manifest_version": 3,
  "name": "My Chrome Extension",
  "version": "1.0",
  "icons": {
    "48": "images/icon48.svg",
    "128": "images/icon128.svg",
    "16": "images/icon16.svg"
  },
  "background": {
    "service_worker": "index.js"
  },
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html"
  }
}
